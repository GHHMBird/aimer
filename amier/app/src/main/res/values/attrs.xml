        </attr>

        <attr name="jtbDuration" format="integer|reference" />

        <attr name="jtbTouchMoveRatioValue" format="float|reference" />
        <attr name="jtbBezierControlValue" format="float|reference" />
        <attr name="jtbStretchDistanceRatioValue" format="float|reference" />
        <attr name="jtbBezierScaleRatioValue" format="float|reference" />

        <attr name="jtbMoveToSameStateCallListener" format="boolean|reference" />
        <attr name="jtbDraggable" format="boolean|reference" />
    </declare-styleable>

    <declare-styleable name="GarlandView">
        <attr name="itemStart" format="dimension" />
        <attr name="itemGap" format="dimension" />
    </declare-styleable>

    <declare-styleable name="FunnyBubbleView">
        <!--半径大小-->
        <attr name="FunnyBubbleView_radius" format="dimension" />
        <!--气泡颜色-->
        <attr name="FunnyBubbleView_color" format="color" />
        <!--要显示的文字-->
        <attr name="FunnyBubbleView_text" format="string|reference" />
        <!--文字大小-->
        <attr name="FunnyBubbleView_textSize" format="dimension" />
        <!--文字颜色-->
        <attr name="FunnyBubbleView_textColor" format="color" />
    </declare-styleable>

    <declare-styleable name="SelectableBackgroundView">
        <attr name="sb_normalColor" format="integer" />
        <attr name="sb_selectedColor" format="integer" />
        <attr name="sb_normalTint" format="reference|color" />
        <attr name="sb_selectedTint" format="reference|color" />
        <attr name="sb_size" format="dimension" />
        <attr name="sb_duration" format="integer" />
        <attr name="sb_isCheck" format="boolean" />
